{
  "name": "restaurant",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "setup": "node scripts/setup-sistema.js",
    "setup:env": "node scripts/setup-env.js",
    "setup:ollama": "node scripts/setup-ollama.js",
    "test:gemma": "node scripts/test-gemma-migration.js",
    "db:gen": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:migrate": "drizzle-kit push",
    "docker:build": "docker-compose build --no-cache",
    "docker:up": "docker-compose up -d",
    "docker:start": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "docker:restart": "docker-compose restart",
    "docker:logs": "docker-compose logs -f",
    "docker:status": "docker-compose ps",
    "docker:clean": "docker-compose down -v && docker system prune -f",
    "docker:migrate": "docker-compose exec app pnpm drizzle-kit push",
    "docker:db": "docker-compose exec postgres psql -U postgres -d restaurant",
    "docker:dev": "docker-compose up -d --build",
    "docker:prod": "docker-compose -f docker-compose.prod.yml up -d --build",
    "deploy:hostinger": "bash scripts/deploy-hostinger.sh",
    "create:admin": "node scripts/create-admin.js",
    "create:admin-test": "node scripts/create-admin.js --test",
    "gerar:hash": "node scripts/gerar-hash-sha256.js",
    "backup:db": "docker-compose -f docker-compose.prod.yml exec postgres pg_dump -U postgres restaurant > backup_$(date +%Y%m%d_%H%M%S).sql",
    "logs:prod": "docker-compose -f docker-compose.prod.yml logs -f",
    "status:prod": "docker-compose -f docker-compose.prod.yml ps",
    "health:check": "curl -f http://localhost/nginx-health && curl -f http://localhost/api/session"
  },
